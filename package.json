{
  "name": "@b-legrand/cli-with-esri",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "rimraf dist && npm run build:app",
    "build:app": "rimraf dist/app && ng build --prod",
    "build:app:watch": "ng build --prod --watch",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadlessCI --no-watch --code-coverage",
    "test:browsers": "ng test --browsers=Chrome,Firefox,IE",
    "test:coverage": "ng test --watch=false --single-run --code-coverage",
    "lint": "ng lint",
    "tslint": "tslint src/*.ts -p tsconfig.app.json -t verbose -t json -o issues.json",
    "tslint:fix": "tslint --fix src/**/*.ts -t verbose -p tsconfig.app.json",
    "tslint:html": "ts-node ./tslintReporter.ts",
    "browser-sync": "npm run build:app && browser-sync start --server --ss ./dist/app/",
    "e2e": "ng e2e",
    "e2e:ci": "ng test --capabilities=ChromeHeadlessCI",
    "e2e:debug": "node --inspect-brk ./node_modules/bin/protractor ./protractor.conf.js",
    "e2e:report": "node cucumberReport.js",
    "webdriver:update": "webdriver-manager update",
    "doc": "compodoc -p tsconfig.app.json",
    "doc:serve": "compodoc -p tsconfig.app.json -s",
    "precommit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular/animations": "^10.0.7",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "^10.0.7",
    "@angular/compiler": "^10.0.7",
    "@angular/core": "^10.0.7",
    "@angular/forms": "^10.0.7",
    "@angular/platform-browser": "^10.0.7",
    "@angular/platform-browser-dynamic": "^10.0.7",
    "@angular/router": "^10.0.7",
    "@angular/service-worker": "^10.0.7",
    "@angularclass/hmr": "^2.1.3",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@types/moment": "^2.13.0",
    "angular-resizable-element": "^3.3.3",
    "angular-split": "^4.0.0",
    "angular2-draggable": "^2.3.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.5",
    "dexie": "^3.0.2",
    "esri-loader": "^2.15.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.19",
    "lunr": "^2.3.8",
    "marked": "^1.1.1",
    "mermaid": "^8.6.4",
    "moment": "^2.27.0",
    "ngx-cookie-service": "^10.0.1",
    "ngx-localstorage": "^3.1.0",
    "ngx-markdown": "^10.1.0",
    "ngx-perfect-scrollbar": "^10.0.0",
    "ngx-popper": "^7.0.0",
    "primeng": "^9.1.3",
    "prismjs": "^1.20.0",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.6.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "^10.0.7",
    "@angular/language-service": "^10.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@ngrx/schematics": "^9.2.0",
    "@types/arcgis-js-api": "^4.16.0",
    "@types/cucumber": "^6.0.1",
    "@types/geojson": "^7946.0.2",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "html-beautify": "^1.0.4",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-expect": "^4.0.3",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.2.2",
    "jest-html-reporter": "^3.1.3",
    "jest-preset-angular": "^8.2.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.9.0",
    "tslint-html-report": "^2.2.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.9.7",
    "webdriver-manager": "^12.1.7"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.ts"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "coverageDirectory": "coverage/",
    "coverageReporters": [
      "lcov",
      "json",
      "html",
      "text"
    ]
  },
  "ngPackage": {
    "dest": "dist/lib",
    "workingDirectory": ".ng_build",
    "lib": {
      "entryFile": "public_api.ts",
      "externals": {
        "@angular/animations": "ng.animations",
        "@ngrx/store": "ngrx.store",
        "@ngrx/router-store": "ngrx.routerStore"
      }
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.ts": [
      "prettier --parser typescript --write --print-width 120 --trailing-comma all",
      "tslint --fix -t verbose --project . --config ./tsconfig.app.json --force",
      "git add"
    ],
    "**/*.html": [
      "html-beautify --wrap-attributes=force-aligned --wrap-line-length=120 --end-with-new-line",
      "git add"
    ]
  }
}
