{
  "name": "@b-legrand/cli-with-esri",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "rimraf dist && npm run build:app",
    "build:app": "rimraf dist/app && ng build --prod",
    "build:app:watch": "ng build --prod --watch",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadlessCI --no-watch --code-coverage",
    "test:browsers": "ng test --browsers=Chrome,Firefox,IE",
    "test:coverage": "ng test --watch=false --single-run --code-coverage",
    "lint": "ng lint",
    "tslint": "tslint src/*.ts -p tsconfig.app.json -t verbose -t json -o issues.json",
    "tslint:fix": "tslint --fix src/**/*.ts -t verbose -p tsconfig.app.json",
    "tslint:html": "ts-node ./tslintReporter.ts",
    "browser-sync": "npm run build:app && browser-sync start --server --ss ./dist/app/",
    "e2e": "ng e2e",
    "e2e:ci": "ng test --capabilities=ChromeHeadlessCI",
    "e2e:debug": "node --inspect-brk ./node_modules/bin/protractor ./protractor.conf.js",
    "e2e:report": "node cucumberReport.js",
    "webdriver:update": "webdriver-manager update",
    "doc": "compodoc -p tsconfig.app.json",
    "doc:serve": "compodoc -p tsconfig.app.json -s",
    "precommit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@angular/service-worker": "^9.1.12",
    "@angularclass/hmr": "^2.1.3",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@types/moment": "^2.13.0",
    "angular-resizable-element": "^3.2.4",
    "angular-split": "^3.0.3",
    "angular2-draggable": "^2.2.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "dexie": "^2.0.2",
    "esri-loader": "^2.2.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.5",
    "lunr": "^2.1.6",
    "marked": "^0.3.17",
    "mermaid": "^7.1.2",
    "moment": "^2.21.0",
    "ngx-cookie-service": "^2.1.0",
    "ngx-localstorage": "^2.0.1",
    "ngx-markdown": "^9.1.1",
    "ngx-perfect-scrollbar": "^7.2.1",
    "ngx-popper": "^6.0.7",
    "primeng": "^9.1.3",
    "prismjs": "^1.12.2",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.1.0",
    "tslib": "^1.10.0",
    "uuid": "^3.2.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@compodoc/compodoc": "^1.0.7",
    "@ngrx/schematics": "^9.2.0",
    "@types/arcgis-js-api": "^4.6.0",
    "@types/cucumber": "^4.0.1",
    "@types/geojson": "^7946.0.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cucumber": "^4.0.0",
    "cucumber-html-reporter": "^4.0.2",
    "html-beautify": "^1.0.4",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-expect": "^3.8.3",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.5.0",
    "jest-html-reporter": "^2.0.0",
    "jest-preset-angular": "^7.0.1",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "protractor": "^5.1.2",
    "protractor-cucumber-framework": "^4.2.0",
    "ts-node": "~4.1.0",
    "tslint": "^5.14.0",
    "tslint-angular": "^1.1.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-html-report": "^1.0.7",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.7.5",
    "webdriver-manager": "^12.0.6"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.ts"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "coverageDirectory": "coverage/",
    "coverageReporters": [
      "lcov",
      "json",
      "html",
      "text"
    ]
  },
  "ngPackage": {
    "dest": "dist/lib",
    "workingDirectory": ".ng_build",
    "lib": {
      "entryFile": "public_api.ts",
      "externals": {
        "@angular/animations": "ng.animations",
        "@ngrx/store": "ngrx.store",
        "@ngrx/router-store": "ngrx.routerStore"
      }
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.ts": [
      "prettier --parser typescript --write --print-width 120 --trailing-comma all",
      "tslint --fix -t verbose --project . --config ./tsconfig.app.json --force",
      "git add"
    ],
    "**/*.html": [
      "html-beautify --wrap-attributes=force-aligned --wrap-line-length=120 --end-with-new-line",
      "git add"
    ]
  }
}
