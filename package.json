{
  "name": "@sncf/socle-common",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "rimraf dist && npm run build:app & npm run build:lib",
    "build:app": "rimraf dist/app && ng build --prod",
    "build:app:watch": "ng build --prod --watch",
    "build:lib": "rimraf dist/lib && ng-packagr -p package.json",
    "test": "ng test",
    "test:gitlab": "ng test --browsers=Chrome_CI",
    "test:browsers": "ng test --browsers=Chrome,Firefox,IE",
    "test:coverage": "ng test --watch=false --single-run --code-coverage",
    "lint": "ng lint",
    "tslint": "tslint src/*.ts -p src/tsconfig.app.json -t verbose -t json -o issues.json",
    "tslint-fix": "tslint --fix src/*.ts -t verbose -p src/tsconfig.app.json",
    "browser-sync": "npm run build:app && browser-sync start --server --ss ./dist/app/",
    "e2e": "ng e2e",
    "e2e:gitlab": "ng test --capabilities=Chrome_CI",
    "e2e:debug": "node --inspect-brk ./node_modules/bin/protractor ./protractor.conf.js",
    "e2e:report": "node cucumberReport.js",
    "webdriver:update": "webdriver-manager update",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@angular/service-worker": "^5.2.7",
    "@angularclass/hmr": "^2.1.3",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@types/moment": "^2.13.0",
    "angular-font-awesome": "^3.1.2",
    "angular-resizable-element": "^2.0.0",
    "angular-split": "^1.0.0-rc.3",
    "angular2-draggable": "^1.2.1",
    "bootstrap": "^4.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "dexie": "^2.0.2",
    "esri-loader": "^2.2.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.5",
    "lunr": "^2.1.6",
    "marked": "^0.3.17",
    "mermaid": "^7.1.2",
    "moment": "^2.21.0",
    "ng-bootstrap": "^1.6.3",
    "primeng": "^5.2.0",
    "prismjs": "^1.12.2",
    "redux": "^3.7.2",
    "rxjs": "^5.5.6",
    "uuid": "^3.2.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.2",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@compodoc/compodoc": "^1.0.7",
    "@types/arcgis-js-api": "^4.6.0",
    "@types/cucumber": "^4.0.1",
    "@types/geojson": "^7946.0.2",
    "@types/jest": "^22.1.4",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "cucumber": "^4.0.0",
    "cucumber-html-reporter": "^4.0.2",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.4.2",
    "jest-html-reporter": "^2.0.0",
    "jest-preset-angular": "^5.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.1.0",
    "prettier": "^1.11.1",
    "protractor": "^5.1.2",
    "protractor-cucumber-framework": "^4.2.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-angular": "^1.1.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-sonarts": "^1.6.0",
    "typescript": "~2.7.0",
    "webdriver-manager": "^12.0.6"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.ts",
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "coverageDirectory": "coverage/",
    "coverageReporters": [
      "lcov",
      "json",
      "html",
      "text"
    ]
  },
  "ngPackage": {
    "dest": "dist/lib",
    "workingDirectory": ".ng_build",
    "lib": {
      "entryFile": "public_api.ts"
    }
  }
}
